<launch>
    <node pkg='joy' type='joy_node' name='joy_to_ros' >
        <param name="dev" value="/dev/input/js0" />
    </node>
    
    <node pkg='joystick_arm_control' type='joy_publisher_node.py' name='joy_to_arm' />
    <node pkg='rosserial_python' type='serial_node.py' name='arm_to_arduino' >
        <param name = "~port" value = "/dev/ttyACM0" />
    </node>
    <!--node pkg='joystick_arm_control' type='joy_to_serial.py' name='ros_to_serial' /-->

</launch>